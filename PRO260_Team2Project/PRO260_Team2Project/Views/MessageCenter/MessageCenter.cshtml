@using PRO260_Team2Project
@model IEnumerable<PRO260_Team2Project.Models.Notification>

@{
    ViewBag.Title = "Your Message Center";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MessageCenter</title>
</head>
<body>
    <div>
        @if (Model.Count() == 0)
        {
            <div>No News</div>
        }else{
        foreach (PRO260_Team2Project.Models.Notification c in Model)
        {
                <div>
                @Html.Partial("~/Views/shared/_NotificationView.cshtml",c)
                </Div>
        }
        }
    </div>
</body>
</html>
