@model IEnumerable<PRO260_Team2Project.Comment>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MessageCenter</title>
</head>
<body>
    <div>
